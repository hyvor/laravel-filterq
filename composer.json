    "autoload-dev": {
        "psr-4": {
          "Hyvor\\FilterQ\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "orchestra/testbench": "^6.0",
        "phpstan/phpstan": "^1.5"
