{
    "name": "hyvor/laravel-filterq",
    "description": "Advanced filtering for Laravel APIs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SupunKavinda",
            "email": "supun@hyvor.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hyvor\\FilterQ\\": "src/"
        }
